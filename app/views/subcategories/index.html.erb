<h1>Subcategories</h1>

<table>
  <th>Name</th>
  <th>Description</th>
  <th>Active</th>
  <th></th>

  <%@subcategories.each do |subcategory|%>
      <tr>
        <td> <%= subcategory.name  %> </td>
        <td><%= subcategory.description%>
        <td><%= subcategory.active%></td>
        <td> <%=link_to "edit", edit_subcategory_url(subcategory.id) %></td>
      </tr>
  <%end%>

</table>