<div id="team_users_table">

<table>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Admin?</th>
  <th></th>

  <%@team_users.each do |user|%>
      <tr data-id="<%= user.id %>">

        <td> <%= user.first_name %> </td>
        <td> <%= user.last_name %> </td>
        <td> <%= user.email %></td>
        <td><%= user.admin? ? "yes" : "no" %>
        <td> <%=link_to "edit", edit_user_registration_path(user.id) %>   /  <%= link_to "delete", "", class: "delete_user" %></td>

      </tr>
  <%end%>

</table>
</div>