<hr>

<h3> Users for Team <%= @team.name %> </h3>

<table>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Admin?</th>
  <th></th>

  <%@team_users.each do |user|%>
      <tr>
        <td> <%= user.first_name %> </td>
        <td> <%= user.last_name %> </td>
        <td> <%= user.email %></td>
        <td><%= user.admin? ? "yes" : "no" %>
        <td> <%=link_to "edit", edit_user_registration_path(user.id) %></td>
      </tr>
  <%end%>

</table>